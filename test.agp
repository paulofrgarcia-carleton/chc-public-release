

      SUBGRAPH(test)
        DATUM(dummy);
        INPUT(dummy);
        CONST(x1,1);
        CONST(x2,2);
        CONST(x3,3);
        CONST(x4,4);
        OUTPUT(x1);
        OUTPUT(x2);
        OUTPUT(x3);
